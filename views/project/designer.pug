extends ../layout

block content
  h4(class='main-title') Select a designer
  div(class='project-list')
    div(class='row')
      div(class='col-sm-8 col-sm-push-2 col-md-6 col-md-push-3')
        div(class='card designer')
          div(class='card-content')
            span(class='pull-right rating')
              i(class='material-icons') star
              i(class='material-icons') star
              i(class='material-icons') star
              i(class='material-icons') star_half
              i(class='material-icons') star_border
              span 3.5
            a(href='/project/1/status' class='card-title') Tim Baggins
            p 15 years of experience in construction industry and now working as an interior designer.
            div(class='row grid-sm' style='margin-bottom:0')
              div(class='col-xs-4')
                a(href='/project/1/status' class='thumbnail')
                  img(src='/images/w3-05_fcd08f2af4dc8631c8157af4e909a0ad-1.jpg')
              div(class='col-xs-4')
                a(href='/project/1/status' class='thumbnail')
                  img(src='/images/w3-05_fcd08f2af4dc8631c8157af4e909a0ad-2.jpg')
              div(class='col-xs-4')
                a(href='/project/1/status' class='thumbnail')
